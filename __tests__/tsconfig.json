{
  "compilerOptions": {
    /* Visit https://aka.ms/tsconfig.json to read more about this file */
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "bundler",
    "allowJs": false,
    "isolatedModules": true,
    "outDir": "./lib",
    "noEmit": false,
    "declaration": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": false,
    "jsx": "react-jsx",
    "forceConsistentCasingInFileNames": true,
    "types": ["vitest/globals"]
  },
  "include": [".."]
}
